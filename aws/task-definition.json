{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "blackjack-fargate",
    "containerDefinitions": [
        {
            "name": "web",
            "image": "",
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "command": [
                "/bin/sh -c \"echo hello\""
            ]
        }
    ],
    "networkMode": "awsvpc",
